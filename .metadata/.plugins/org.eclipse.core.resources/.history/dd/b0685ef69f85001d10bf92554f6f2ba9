		int sum=0;
		int cube;
		int size=sc.nextInt();
		//int sum=0;
		//int cube;
		int a[]=new int[size];//3
		for(int i=0;i<size;i++)//0<3 1<3 2<3
		{
			 a[i]=sc.nextInt();
			 cube=a[i]*a[i]*a[i];
				sum=sum+cube;
			 //a[0]=12,a[1]=14 a[2]=15
		}
		//for(int j=0;j<size;j++)
		//{
			//cube=a[i]*a[i]*a[i];
