package classandobject;

public class Student1 {
	 private int id;
	private String name;
	 private String Address;
	
   private float sub1;
  private  float sub2;
  private  float sub3;
  private  float sub4;
  private  float sub5;
    public Student1(int i,String s,String j)
    {
    	id=i;
    	name=s;
    	Address=j;
    	//sub1=a;
    	//sub2=b;
    	//sub3=c;
    	//sub4=d;
    	//sub5=e;
    	
    }
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAddress() {
		return Address;
	}
	public void setAddress(String address) {
		Address = address;
	}

	
	  double total()
	    {
	    	return (sub1+sub2+sub3+sub4+sub5);

	}
	    double percentage()
	    {
	    	return((total()/500)*100);
	    	
	    }
		@Override
		public String toString() {
			return "Student1 [id=" + id + ", name=" + name + ", Address=" + Address + ", sub1=" + sub1 + ", sub2="
					+ sub2 + ", sub3=" + sub3 + ", sub4=" + sub4 + ", sub5=" + sub5 + "]";
		}
	    
}
