package Assignment1;

public class StarProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
      // int i;
       for(int i=1;i<=5;i++);
       {
    	   for(int j=1;j<=i;j++)
    	   {
    	   System.out.print("*");
       }
    	   System.out.println();
    	   
    	 
	}

}
}